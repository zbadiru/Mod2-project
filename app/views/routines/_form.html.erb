<%= form_for @routine do |f|%>

    <%= f.label :name %>
    <%= f.text_field :name %><br>
    
    <%= f.label :personal_trainer %>
    <%= f.collection_select :personal_trainer_id, PersonalTrainer.all, :id, :name, prompt: true%> <br>

    <%= f.fields_for :exercises do |exe| %>
        <%= exe.label :name%>
        <%= exe.text_field :name%><br>

    <% end%>
    <%= f.label :repetitions %>
    <%= f.number_field :repetitions %><br>

    <%= f.submit %>
    <% end %>