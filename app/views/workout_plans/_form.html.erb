
<%= form_for @workout_plan do |f|%>

    <%= f.label :routine %>
    <%= f.collection_select :routine_id, Routine.all, :id, :name, prompt: true%> <br>

    <%= f.label :personal_trainer %>
    <%= f.collection_select :personal_trainer_id, PersonalTrainer.all, :id, :name, prompt: true%> <br>

    <% #need a routine drop down option %>

    <%= f.submit %>
<% end %>